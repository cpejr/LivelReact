{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "project": "./jsconfig.json",
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "react/prop-types": 0,
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": 1,
        "react-hooks/exhaustive-deps": 0
    },
    "plugins": [
        "react",
        "react-hooks"
    ]
}
